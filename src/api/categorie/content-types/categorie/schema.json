{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "categorie",
    "pluralName": "categories",
    "displayName": "Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "category_name": {
      "type": "string"
    },
    "subcategories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subcategory.subcategory",
      "mappedBy": "category"
    },
    "slug": {
      "type": "uid"
    },
    "meta_title": {
      "type": "string"
    },
    "meta_desc": {
      "type": "string"
    },
    "sort_order": {
      "type": "integer"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "category"
    },
    "cover_image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
